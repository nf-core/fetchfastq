nextflow_process {

    name "Test Process MULTIQC_MAPPINGS_CONFIG"
    script "modules/local/multiqc_mappings_config.nf"
    process "MULTIQC_MAPPINGS_CONFIG"
    tag "multiqc_mappings_config"
    tag "modules_local"

    test("SRX9626017_SRR13191702") {

        when {
            params {
                outdir   = "$outputDir"
            }
            process {
                """
                input[0] = file("$baseDir/modules/local/tests/input/multiqc_mappings_config/SRX9626017_SRR13191702.mappings.csv", checkIfExists: true)
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() },
            )
        }

    }

}
